def find_character_index(string, char):
    for i in range(len(string)):
        if string[i] == char:
            return i
    return -1

# Get input from the user
input_string = input("Enter a string: ")
search_char = input("Enter the character to search: ")

# Find the index of the character
result = find_character_index(input_string, search_char)

# Display the result
if result != -1:
    print(f"The character '{search_char}' is present at index {result}.")
else:
    print(f"The character '{search_char}' is not present in the string.")
